{
  "name": "snooze-vue-storefront",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern",
    "start": "nuxt start --modern",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "storybook": "nuxt storybook",
    "generate:fragmentsTypesShopify": "node ./app/shopify/fragments-extractor.js",
    "generate:fragmentsTypesPrismic": "node ./app/prismic/fragments-extractor.js",
    "generate:shopify": "node ./app/static/shopify-settings.js",
    "generate:algolia": "node ./app/static/algolia-settings.js",
    "generate:files": "run-p generate:shopify generate:algolia generate:fragmentsTypesShopify generate:fragmentsTypesPrismic",
    "pre:commit": "run-s lint lintfix",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/proxy": "^2.0.1",
    "@prismicio/vue": "^2.0.7",
    "@sendgrid/mail": "^7.4.2",
    "algoliasearch": "^4.8.3",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-prismic": "^1.0.8",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.8.2",
    "graphql-tag": "^2.11.0",
    "intersection-observer": "^0.12.0",
    "nuxt": "2.14.12",
    "path-to-regexp": "^6.2.0",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^3.0.2",
    "qs": "^6.9.6",
    "ramda": "^0.27.1",
    "store": "^2.0.12",
    "swiper": "^6.3.5",
    "uuid": "^8.3.1",
    "vue-awesome-swiper": "^4.1.1",
    "vue-click-outside": "^1.1.0",
    "vue-instantsearch": "^3.4.3",
    "vue-observe-visibility": "^1.0.0",
    "vue-recaptcha": "^1.3.0",
    "vue-slider-component": "^3.2.11",
    "vue2-perfect-scrollbar": "^1.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.17",
    "@nuxtjs/device": "^2.0.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/storybook": "^3.3.0",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@storybook/addon-knobs": "^6.1.14",
    "@tailwindcss/typography": "^0.2.0",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-custom-properties": "^11.0.0",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre:commit"
    }
  }
}
